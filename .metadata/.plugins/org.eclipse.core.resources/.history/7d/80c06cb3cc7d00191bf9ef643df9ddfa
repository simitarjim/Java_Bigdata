package Exam04;


public class Pursuer {

	public int pretty;
	public int ugly;
	Girl[] totalgirls;
	Pursuer girls;
	
	public void girls() {
		int[] cookgirls = new int[pretty+ugly];
		for (int i = 0; i < totalgirls.length; i++) {
			if (totalgirls[i].cook == true) {
				System.out.println(totalgirls[i]);
			}
			
		}
		
	}
	

}
