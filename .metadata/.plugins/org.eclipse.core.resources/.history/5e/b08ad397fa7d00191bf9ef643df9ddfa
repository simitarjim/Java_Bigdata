package Exam04;

public class No09 {
	public static void main(String[] args) {
		
		
		//数据基本信息 从网络中读取
		ProcessData datafromweb = new ProcessData();
		datafromweb.name = "bigdata";
		datafromweb.path = "www.zuikc.com";
		//数据从硬盘中读取
		ProcessData datafromdriver = new ProcessData();
		datafromdriver.name = "java";
		datafromdriver.path = "c:";
		//数据从内存上读取
		ProcessData datafrommemmory new ProcessData();
		datafrommemmory.name = "java1";
		datafrommemmory.path = "heap";
	}

}


