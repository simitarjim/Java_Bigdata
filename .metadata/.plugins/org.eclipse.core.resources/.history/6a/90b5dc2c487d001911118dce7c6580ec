package Exam04;

public class Pursuer {

	public int pretty;
	public int ugly;
	public Girl[]  totalgirls;
	public Pursuer girls;
	
	public void married() {
		System.out.println("美女一共有"+girls.pretty+"位");
		System.out.println("丑女一共有"+girls.ugly+"位");
		int[] cookgirl = new int[totalgirls.length];
		
		for (int i = 0; i < totalgirls.length; i++) {
			if (totalgirls[i].cook == true) {
				cookgirl[i] = totalgirls[i];
				
			}
			
		}
		
	}

}
